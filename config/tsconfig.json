{
  "compilerOptions": {
    "incremental": true,
    "isolatedModules": true,
    "module": "node18",
    "moduleResolution": "node16",
    "noUncheckedSideEffectImports": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "extends": "tsconfig-seek"
}
