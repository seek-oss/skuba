{
  "app": "npx ts-node infra/index.ts",
  "context": {
    "@aws-cdk/core:enableStackNameDuplicates": "true",
    "global": {
      "appName": "<%- serviceName %>"
    },
    "dev": {
      "workerLambda": {
        "reservedConcurrency": 1,
        "environment": {
          "SOMETHING": "dev"
        }
      }
    },
    "prod": {
      "workerLambda": {
        "reservedConcurrency": 2,
        "environment": {
          "SOMETHING": "prod"
        }
      }
    }
  }
}
