{
  "name": "<%- moduleName %>",
  "version": "0.0.0-semantically-released",
  "description": "<%- description %>",
  "imports": {
    "#src/*": {
      "@seek/<%- serviceName %>/source": "./src/*",
      "default": "./lib/*"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/<%- orgName %>/<%- repoName %>.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "commit": "cz",
    "format": "skuba format",
    "lint": "skuba lint",
    "release": "pnpm --silent build && skuba release",
    "test": "skuba test",
    "test:ci": "skuba test --coverage",
    "test:watch": "skuba test --watch"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^22.13.10",
    "commitizen": "^4.2.4",
    "skuba": "*",
    "tsdown": "^0.18.0"
  },
  "packageManager": "pnpm@10.7.0",
  "engines": {
    "node": ">=20.9.0"
  }
}
