{
  "name": "@seek/<%- serviceName %>",
  "private": true,
  "license": "UNLICENSED",
  "imports": {
    "#src/*": {
      "types": "./src/*",
      "import": "./lib/*",
      "require": "./lib/*"
    }
  },
  "scripts": {
    "build": "skuba build",
    "format": "skuba format",
    "lint": "skuba lint",
    "start": "skuba start --port <%- port %>",
    "start:debug": "pnpm --silent start --inspect-brk",
    "test": "skuba test",
    "test:ci": "skuba test --coverage",
    "test:watch": "skuba test --watch"
  },
  "dependencies": {
    "@seek/logger": "^10.0.0",
    "express": "^5.0.0",
    "hot-shots": "^10.0.0",
    "seek-datadog-custom-metrics": "^4.6.3",
    "skuba-dive": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "@types/supertest": "^6.0.0",
    "mime": "^4.0.1",
    "pino-pretty": "^13.0.0",
    "skuba": "*",
    "supertest": "^7.0.0"
  },
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": ">=22"
  }
}
