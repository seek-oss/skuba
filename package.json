{
  "name": "skuba",
  "description": "Toolkit for backend TypeScript development",
  "bin": {
    "skuba": "lib/skuba.js"
  },
  "engines": {
    "node": ">=12"
  },
  "version": "3.11.0",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "config/**/*",
    "lib*/**/*.d.ts",
    "lib*/**/*.js",
    "lib*/**/*.js.map",
    "template/**/*",
    "jest-preset.js"
  ],
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "build": "yarn skuba build && scripts/postbuild.sh",
    "format": "yarn skuba format",
    "lint": "yarn skuba lint",
    "release": "yarn build && changeset publish",
    "stage": "changeset version && yarn format",
    "skuba": "ts-node src/skuba",
    "test": "yarn skuba test",
    "test:watch": "yarn skuba test --watch",
    "test:template": "scripts/test-template.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/skuba.git"
  },
  "devDependencies": {
    "@changesets/cli": "2.12.0",
    "@types/concurrently": "5.2.1",
    "@types/ejs": "3.0.5",
    "@types/express": "4.17.9",
    "@types/fs-extra": "9.0.6",
    "@types/koa": "2.11.6",
    "@types/lodash.mergewith": "4.6.6",
    "@types/module-alias": "2.0.0",
    "@types/npm-which": "3.0.0",
    "@types/supertest": "2.0.10",
    "express": "4.17.1",
    "koa": "2.13.1",
    "supertest": "6.0.1",
    "type-fest": "0.20.2"
  },
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.19",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "chalk": "^4.1.0",
    "concurrently": "^5.3.0",
    "ejs": "^3.1.5",
    "enquirer": "^2.3.6",
    "eslint": "^7.16.0",
    "eslint-config-skuba": "1.0.10",
    "execa": "^5.0.0",
    "fs-extra": "^9.0.1",
    "get-port": "^5.1.1",
    "ignore": "^5.1.8",
    "is-installed-globally": "^0.3.2",
    "jest": "^26.6.3",
    "latest-version": "^5.1.0",
    "lodash.mergewith": "^4.6.2",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.6",
    "normalize-package-data": "^3.0.0",
    "npm-run-path": "^4.0.1",
    "npm-which": "^3.0.1",
    "prettier": "2.2.1",
    "read-pkg-up": "^7.0.1",
    "runtypes": "^5.0.1",
    "semantic-release": "^17.3.1",
    "serialize-error": "^8.0.0",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "1.1.1",
    "tsconfig-seek": "1.0.2",
    "typescript": "4.1.3",
    "yargs-parser": "^20.2.4"
  },
  "peerDependencies": {
    "@types/jest": ">= 25 < 27",
    "skuba-dive": "1"
  },
  "peerDependenciesMeta": {
    "skuba-dive": {
      "optional": true
    },
    "@types/jest": {
      "optional": true
    }
  }
}
