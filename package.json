{
  "name": "skuba",
  "version": "5.0.1",
  "private": false,
  "description": "SEEK development toolkit for backend applications and packages",
  "homepage": "https://github.com/seek-oss/skuba#readme",
  "bugs": {
    "url": "https://github.com/seek-oss/skuba/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/seek-oss/skuba.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "bin": {
    "skuba": "lib/skuba.js"
  },
  "files": [
    "config/**/*",
    "jest/**/*",
    "lib*/**/*.d.ts",
    "lib*/**/*.js",
    "lib*/**/*.js.map",
    "template/**/*",
    "jest-preset.js"
  ],
  "scripts": {
    "build": "scripts/build.sh",
    "deploy": "scripts/deploy.sh",
    "format": "yarn skuba format",
    "lint": "yarn skuba lint && yarn lint:md",
    "lint:md": "remark --frail --quiet .",
    "release": "yarn build && changeset publish",
    "skuba": "yarn build && node lib/skuba",
    "stage": "changeset version && yarn format",
    "test": "yarn skuba test",
    "test:ci": "yarn skuba test --config jest.config.int.ts --runInBand",
    "test:int": "yarn skuba test --config jest.config.int.ts --runInBand",
    "test:template": "scripts/test-template.sh",
    "test:watch": "yarn skuba test --config jest.config.int.ts --runInBand --watch"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "remark-lint-list-item-indent",
        false
      ],
      [
        "remark-lint-no-shortcut-reference-link",
        false
      ],
      [
        "remark-lint-no-undefined-references",
        {
          "allow": [
            " ",
            "x"
          ]
        }
      ]
    ]
  },
  "resolutions": {
    "**/@types/node": ">=14.18"
  },
  "dependencies": {
    "@esbuild-plugins/tsconfig-paths": "^0.0.4",
    "@jest/types": "^29.0.0",
    "@octokit/graphql": "^5.0.0",
    "@octokit/graphql-schema": "^12.0.0",
    "@octokit/rest": "^19.0.0",
    "@octokit/types": "^8.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": ">=14.18",
    "chalk": "^4.1.0",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.6",
    "enquirer": "^2.3.6",
    "esbuild": "~0.15.7",
    "eslint": "^8.11.0",
    "eslint-config-skuba": "1.2.1",
    "execa": "^5.0.0",
    "fdir": "^5.0.0",
    "fs-extra": "^11.0.0",
    "function-arguments": "^1.0.9",
    "get-port": "^5.1.1",
    "ignore": "^5.1.8",
    "is-installed-globally": "^0.4.0",
    "isomorphic-git": "^1.11.1",
    "jest": "^29.0.1",
    "jest-watch-typeahead": "^2.1.1",
    "libnpmsearch": "^6.0.0",
    "lodash.mergewith": "^4.6.2",
    "minimist": "^1.2.6",
    "normalize-package-data": "^5.0.0",
    "npm-run-path": "^4.0.1",
    "npm-which": "^3.0.1",
    "picomatch": "^2.2.2",
    "prettier": "~2.8.0",
    "read-pkg-up": "^7.0.1",
    "runtypes": "^6.0.0",
    "semantic-release": "^19.0.0",
    "serialize-error": "^8.0.1",
    "simple-git": "^3.5.0",
    "sort-package-json": "^1.57.0",
    "strip-ansi": "^6.0.1",
    "ts-dedent": "^2.2.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "tsconfig-seek": "1.0.2",
    "typescript": "~4.9.0",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "2.25.2",
    "@changesets/get-github-info": "0.5.1",
    "@jest/reporters": "29.3.1",
    "@types/ejs": "3.1.1",
    "@types/express": "4.17.14",
    "@types/fs-extra": "9.0.13",
    "@types/koa": "2.13.5",
    "@types/libnpmsearch": "2.0.3",
    "@types/lodash.mergewith": "4.6.7",
    "@types/module-alias": "2.0.1",
    "@types/npm-which": "3.0.1",
    "@types/picomatch": "2.3.0",
    "@types/supertest": "2.0.12",
    "@types/validate-npm-package-name": "4.0.0",
    "enhanced-resolve": "5.12.0",
    "express": "4.18.2",
    "jsonfile": "6.1.0",
    "koa": "2.13.4",
    "memfs": "3.4.12",
    "remark-cli": "11.0.0",
    "remark-preset-lint-recommended": "6.1.2",
    "semver": "7.3.8",
    "supertest": "6.3.2",
    "type-fest": "2.19.0"
  },
  "peerDependencies": {
    "skuba-dive": "1 || 2"
  },
  "peerDependenciesMeta": {
    "skuba-dive": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14.18"
  },
  "skuba": {
    "build": "esbuild",
    "entryPoint": "src/index.ts",
    "template": null,
    "type": "package",
    "version": "4.0.0"
  }
}
