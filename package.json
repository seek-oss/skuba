{
  "name": "skuba",
  "description": "Toolkit for backend TypeScript development",
  "bin": {
    "skuba": "lib-commonjs/skuba.js"
  },
  "engines": {
    "node": ">=12"
  },
  "version": "3.6.0",
  "main": "lib-commonjs",
  "module": "lib-es2015",
  "typings": "lib-types/index.d.ts",
  "files": [
    "config/**/*",
    "lib*/**/*.d.ts",
    "lib*/**/*.js",
    "lib*/**/*.js.map",
    "template/**/*",
    "jest-preset.js"
  ],
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "build": "yarn skuba build-package && scripts/postbuild.sh",
    "format": "yarn skuba format",
    "lint": "yarn skuba lint",
    "release": "yarn build && changeset publish",
    "stage": "changeset version && yarn format",
    "skuba": "ts-node src/skuba",
    "test": "jest",
    "test:template": "scripts/test-template.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/skuba.git"
  },
  "devDependencies": {
    "@changesets/cli": "2.8.0",
    "@types/concurrently": "5.2.1",
    "@types/ejs": "3.0.4",
    "@types/fs-extra": "9.0.1",
    "@types/lodash": "4.14.155",
    "@types/npm-which": "3.0.0",
    "type-fest": "0.15.1"
  },
  "dependencies": {
    "@types/jest": "^26.0.0",
    "chalk": "^4.1.0",
    "concurrently": "^5.2.0",
    "ejs": "^3.1.3",
    "enquirer": "^2.3.5",
    "eslint": "^7.2.0",
    "eslint-config-seek": "^7.0.1",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-tsdoc": "^0.2.5",
    "execa": "^4.0.2",
    "fs-extra": "^9.0.1",
    "get-port": "^5.1.1",
    "ignore": "^5.1.8",
    "jest": "^26.0.1",
    "latest-version": "^5.1.0",
    "lodash": "^4.17.15",
    "normalize-package-data": "^2.5.0",
    "npm-run-path": "^4.0.1",
    "npm-which": "^3.0.1",
    "prettier": "2.0.5",
    "read-pkg-up": "^7.0.1",
    "runtypes": "^4.2.0",
    "semantic-release": "^17.0.8",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-seek": "1.0.2",
    "typescript": "3.9.5"
  },
  "peerDependencies": {
    "@types/jest": ">= 25 < 27",
    "skuba-dive": "1"
  },
  "peerDependenciesMeta": {
    "skuba-dive": {
      "optional": true
    },
    "@types/jest": {
      "optional": true
    }
  }
}
