{
  "name": "skuba",
  "description": "Toolkit for backend TypeScript development",
  "bin": {
    "skuba": "lib-commonjs/skuba.js"
  },
  "engines": {
    "node": ">=12"
  },
  "version": "3.10.1",
  "main": "lib-commonjs",
  "module": "lib-es2015",
  "typings": "lib-types/index.d.ts",
  "files": [
    "config/**/*",
    "lib*/**/*.d.ts",
    "lib*/**/*.js",
    "lib*/**/*.js.map",
    "template/**/*",
    "jest-preset.js"
  ],
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "build": "yarn skuba build-package && scripts/postbuild.sh",
    "format": "yarn skuba format",
    "lint": "yarn skuba lint",
    "release": "yarn build && changeset publish",
    "stage": "changeset version && yarn format",
    "skuba": "ts-node src/skuba",
    "test": "yarn skuba test",
    "test:template": "scripts/test-template.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/skuba.git"
  },
  "devDependencies": {
    "@changesets/cli": "2.11.2",
    "@types/concurrently": "5.2.1",
    "@types/ejs": "3.0.5",
    "@types/fs-extra": "9.0.4",
    "@types/lodash.mergewith": "4.6.6",
    "@types/npm-which": "3.0.0",
    "type-fest": "0.18.1"
  },
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/jest": "^26.0.15",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "chalk": "^4.1.0",
    "concurrently": "^5.3.0",
    "ejs": "^3.1.5",
    "enquirer": "^2.3.6",
    "eslint": "^7.11.0",
    "eslint-config-skuba": "1.0.8",
    "execa": "^4.0.3",
    "fs-extra": "^9.0.1",
    "get-port": "^5.1.1",
    "ignore": "^5.1.8",
    "jest": "^26.6.0",
    "latest-version": "^5.1.0",
    "lodash.mergewith": "^4.6.2",
    "nodemon": "^2.0.6",
    "normalize-package-data": "^3.0.0",
    "npm-run-path": "^4.0.1",
    "npm-which": "^3.0.1",
    "prettier": "2.1.2",
    "read-pkg-up": "^7.0.1",
    "runtypes": "^5.0.1",
    "semantic-release": "^17.2.1",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "ts-node-dev": "1.0.0",
    "tsconfig-seek": "1.0.2",
    "typescript": "4.0.5",
    "yargs-parser": "^20.2.3"
  },
  "peerDependencies": {
    "@types/jest": ">= 25 < 27",
    "skuba-dive": "1"
  },
  "peerDependenciesMeta": {
    "skuba-dive": {
      "optional": true
    },
    "@types/jest": {
      "optional": true
    }
  }
}
