{
  "name": "skuba",
  "description": "Toolkit for backend TypeScript development",
  "bin": {
    "skuba": "lib/skuba.js"
  },
  "engines": {
    "node": ">=12"
  },
  "version": "9.99.99",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "config/**/*",
    "lib*/**/*.d.ts",
    "lib*/**/*.js",
    "lib*/**/*.js.map",
    "template/**/*",
    "jest-preset.js"
  ],
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "build": "yarn skuba build && scripts/postbuild.sh",
    "format": "yarn skuba format",
    "lint": "yarn skuba lint",
    "release": "yarn build && changeset publish",
    "stage": "changeset version && yarn format",
    "skuba": "ts-node src/skuba",
    "test": "yarn skuba test",
    "test:watch": "yarn skuba test --watch",
    "test:template": "scripts/test-template.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/skuba.git"
  },
  "devDependencies": {
    "@changesets/cli": "2.16.0",
    "@types/concurrently": "6.2.1",
    "@types/ejs": "3.0.7",
    "@types/express": "4.17.13",
    "@types/fs-extra": "9.0.12",
    "@types/koa": "2.13.4",
    "@types/lodash.mergewith": "4.6.6",
    "@types/module-alias": "2.0.1",
    "@types/npm-which": "3.0.1",
    "@types/picomatch": "2.2.6",
    "@types/supertest": "2.0.11",
    "express": "4.17.1",
    "koa": "2.13.1",
    "supertest": "6.1.4",
    "type-fest": "1.3.0"
  },
  "dependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.23",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "chalk": "^4.1.0",
    "concurrently": "^6.0.0",
    "ejs": "^3.1.6",
    "enquirer": "^2.3.6",
    "eslint": "^7.27.0",
    "eslint-config-skuba": "1.0.11",
    "execa": "^5.0.0",
    "fdir": "^5.0.0",
    "fs-extra": "^10.0.0",
    "get-port": "^5.1.1",
    "ignore": "^5.1.8",
    "is-installed-globally": "^0.4.0",
    "jest": "^27.0.3",
    "latest-version": "^5.1.0",
    "lodash.mergewith": "^4.6.2",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.7",
    "normalize-package-data": "^3.0.0",
    "npm-run-path": "^4.0.1",
    "npm-which": "^3.0.1",
    "picomatch": "^2.2.2",
    "prettier": "2.3.2",
    "read-pkg-up": "^7.0.1",
    "runtypes": "^6.0.0",
    "semantic-release": "^17.3.8",
    "serialize-error": "^8.0.1",
    "source-map-support": "^0.5.19",
    "ts-jest": "^27.0.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "1.1.8",
    "tsconfig-seek": "1.0.2",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "skuba-dive": "1"
  },
  "peerDependenciesMeta": {
    "skuba-dive": {
      "optional": true
    }
  }
}
