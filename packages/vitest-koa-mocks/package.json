{
  "name": "@skuba-lib/vitest-koa-mocks",
  "version": "0.0.0",
  "private": false,
  "description": "Vitest mocks for Koa",
  "homepage": "https://github.com/seek-oss/skuba/tree/main/packages/vitest-koa-mocks#readme",
  "bugs": {
    "url": "https://github.com/seek-oss/skuba/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seek-oss/skuba.git",
    "directory": "packages/vitest-koa-mocks"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "@seek/skuba/source": "./src/index.ts",
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.cts",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.js.map"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "pnpm skuba format",
    "lint": "pnpm skuba lint",
    "skuba": "node ../../lib/skuba",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "node-mocks-http": "^1.17.2"
  },
  "devDependencies": {
    "tsdown": "^0.17.0"
  },
  "peerDependencies": {
    "@types/koa": "^3.0.1",
    "koa": "^3.1.1",
    "vitest": "^4.0.14"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "require": "./lib/index.cjs",
        "import": "./lib/index.mjs"
      },
      "./package.json": "./package.json"
    }
  },
  "skuba": {
    "entryPoint": "src/index.ts",
    "template": "oss-npm-package",
    "type": "package",
    "version": "13.1.1"
  }
}
