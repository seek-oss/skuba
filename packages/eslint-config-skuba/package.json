{
  "name": "eslint-config-skuba",
  "version": "7.0.2",
  "private": false,
  "description": "ESLint config for skuba",
  "homepage": "https://github.com/seek-oss/skuba/tree/main/packages/eslint-config-skuba#readme",
  "bugs": {
    "url": "https://github.com/seek-oss/skuba/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seek-oss/skuba.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "index.d.ts",
    "index.js",
    "requireExtensions.js"
  ],
  "scripts": {
    "format": "pnpm --silent skuba format",
    "lint": "pnpm --silent skuba lint",
    "skuba": "node --experimental-vm-modules ../../lib/skuba"
  },
  "dependencies": {
    "eslint-config-seek": "14.5.3",
    "eslint-plugin-skuba": "workspace:*",
    "eslint-plugin-yml": "^1.14.0",
    "typescript-eslint": "^8.26.0"
  },
  "devDependencies": {
    "eslint": "^9.11.1",
    "typescript": "~5.8.0"
  },
  "peerDependencies": {
    "eslint": ">=9.11.1",
    "typescript": ">=5.5.4"
  },
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=20.9.0"
  },
  "publishConfig": {
    "provenance": true
  },
  "skuba": {
    "entryPoint": "index.js",
    "template": "oss-npm-package",
    "type": "package",
    "version": "12.0.2"
  }
}
